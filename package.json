{
  "name": "project-management",
  "version": "0.0.1",
  "description": "This sample shows how to do write-back for a project management dashboard using the Extensions API.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "lint": "semistandard"
  },
  "author": "Ben Lower",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "semistandard": "^11.0.0"
  },
  "semistandard": {
    "env": {
      "jquery": true,
      "tableau": true
    },
    "globals": [
      "tableau"
    ],
    "ignore": [
      "/lib",
      "/docs",
      "/public/js/tableau-extensions*.js"
    ]
  }
}
